{
  "name": "did-key-test-suite",
  "version": "0.1.0",
  "description": "Template for Creating Tests using the mocha-w3c-interop-reporter",
  "main": "index.js",
  "scripts": {
    "test": "mocha tests/ --require esm --reporter ./node_modules/mocha-w3c-interop-reporter/index.js --reporter-options body=\"$PWD/body.hbs\",matrix=\"$PWD/matrix.hbs\",reportDir=\"$PWD/reports\",respec=\"$PWD/respecConfig.json\",title=\"did:key Method Interoperability Report 1.0\",suiteLog='./suite.log' --timeout 15000",
    "lint": "eslint *.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digitalbazaar/did-key-test-suite.git"
  },
  "keywords": [
    "Test",
    "W3C",
    "VC",
    "Reporter"
  ],
  "author": {
    "name": "Digital Bazaar, Inc.",
    "email": "support@digitalbazaar.com",
    "url": "https://digitalbazaar.com/"
  },
  "engines": {
    "node": "14.0.0"
  },
  "dependencies": {
    "@digitalbazaar/did-method-key": "^2.0.0",
    "@digitalbazaar/http-client": "^2.0.1",
    "chai": "^4.3.4",
    "mocha": "^9.1.3",
    "mocha-w3c-interop-reporter": "github:digitalbazaar/mocha-w3c-interop-reporter#add-more-status-marks",
    "require-dir": "^1.2.0"
  },
  "devDependencies": {
    "eslint": "^7.23.0",
    "eslint-config-digitalbazaar": "^2.6.1",
    "eslint-plugin-jsdoc": "^32.3.0"
  },
  "bugs": {
    "url": "https://github.com/digitalbazaar/did-key-test-suite/issues"
  },
  "homepage": "https://github.com/digitalbazaar/did-key-test-suite#readme"
}
